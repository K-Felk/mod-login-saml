{
  "id": "mod-login-saml",
  "name": "SAML login",
  "provides": [
    {
      "id": "mod-login-saml",
      "version": "0.0.1",
      "handlers": [
        {
          "methods": [
            "GET"
          ],
          "pathPattern": "/saml-login",
          "modulePermissions": [
            "configuration.entries.collection.get"
          ]
        },
        {
          "methods": [
            "POST"
          ],
          "pathPattern": "/saml-callback",
          "modulePermissions": [
            "configuration.entries.collection.get"
          ]
        },
        {
          "methods": [
            "GET"
          ],
          "pathPattern": "/saml-regenerate",
          "modulePermissions": [
            "configuration.entries.collection.get",
            "configuration.entries.item.post",
            "configuration.entries.item.put"
          ]
        },
        {
          "methods": [
            "GET"
          ],
          "pathPattern": "/saml-config-check",
          "modulePermissions": [
            "configuration.entries.collection.get"
          ]
        }
      ]
    }
  ]
}
