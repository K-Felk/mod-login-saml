{
  "$schema": "http://json-schema.org/draft-03/schema#",
  "type": "object",
  "properties": {
    "bindingMethod": {
      "required": true,
      "type": "string",
      "enum": [
        "POST",
        "GET"
      ]
    },
    "location": {
      "required": true,
      "type": "string"
    },
    "samlRequest": {
      "required": false,
      "type": "string"
    },
    "relayState": {
      "required": false,
      "type": "string"
    }
  }
}
